{
  "name": "@shadx/cli",
  "version": "1.0.2",
  "description": "A CLI tool to generate UI components for your project",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "shadx",
    "url": "https://github.com/BelkacemYerfa/shadcn-extension"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BelkacemYerfa/shadcn-extension.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "engines": {
    "node": ">=14.16"
  },
  "paths": {
    "@/*": [
      "./*"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "shadcn",
    "shadcn-extension",
    "shadcn-cli",
    "shadcn-ui",
    "extension-cli",
    "extension-ui",
    "extension",
    "cli",
    "ui",
    "react",
    "typescript",
    "nextjs",
    "tailwindcss"
  ],
  "scripts": {
    "pub:release":"pnpm publish --access public",
    "cli": "node dist/index.js",
    "init": "node dist/index.js init",
    "build": "tsup src/index.ts --format esm --clean --minify --metafile",
    "dev": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.js\"",
    "test": "vitest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "commander": "^9.5.0",
    "execa": "^7.2.0",
    "figlet": "^1.7.0",
    "https-proxy-agent": "^7.0.4",
    "node-fetch": "^3.3.2",
    "ora": "^6.3.1",
    "prompts": "^2.4.2",
    "semver": "^7.6.0",
    "zod": "^3.22.4",
    "glob": "^10.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/chalk-animation": "^1.6.3",
    "@types/figlet": "^1.5.5",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/glob": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "tsup": "^6.5.0",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,yml,json}": [
      "prettier --write"
    ]
  }
}